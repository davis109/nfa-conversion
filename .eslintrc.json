{
    "env": {
        "browser": true,
        "mocha": true
    },
    "extends": "standard",
    "parserOptions": {
        "ecmaVersion": 12,
        "sourceType": "module"
    },
    "rules": {
        "indent": [
            "error",
            4
        ],
        "quote-props": [
            "error",
            "consistent"
        ]
    }
}